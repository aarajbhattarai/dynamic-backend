{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": "Organize your content into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "category"
    },
    "description": {
      "type": "text"
    },
    "job_vacancy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job-vacancy.job-vacancy",
      "inversedBy": "categories"
    },
    "employer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::employer.employer",
      "inversedBy": "categories"
    }
  }
}
